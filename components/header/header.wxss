/* components/header.wxss */
.notLoggedIn{
  background: #27A899;
  display: flex;
  flex-direction: column;
}

.title{
  color:white;
  margin: auto;
  font-size: 32px;
  padding-top: 32px;
  font-weight:600;
}
.text{
  margin: auto;
  color:white;
  padding-top: 24px;
}
.buttonWrapper{
  display: flex;
  padding-top: 24px;
  padding-bottom: 16px;
  justify-content: center;
}

.button{
  font-size: 16px;
  margin: 0;
  color: #27A899;
}
.button:first-child{
  margin-right:8px ;
}
.button:last-child{
  margin-left:8px ;
}
.logged{
  border: 1px solid red;
  background: #27A899;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.title-logged{
  color:white;
  font-size: 32px;
  font-weight:600;
}
.img{
  width: 46px;
}
.create{
  border: 1px solid black;
  width: 46px;
  margin-right:5px ;
}

